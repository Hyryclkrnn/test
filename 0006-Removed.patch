diff --git a/string.c b/string.c
new file mode 100644
index 0000000..814bd0e
--- /dev/null
+++ b/string.c
@@ -0,0 +1,26 @@
+//#include <studio.h>
+int my_strlen(char *s)
+{
+   char *p = s;
+
+   while (*p)
+      ++p;
+
+   return (p - s);
+}
+
+int main(void)
+{
+   int i;
+   char *s[] = 
+   {
+      "Git tutorials",
+      "Tutorials Point"
+   };
+
+   for (i = 0; i < 2; ++i)
+
+  printf("string lenght of %s = %lu\n", s[i], my_strlen(s[i]));
+
+    return 0;
+}
